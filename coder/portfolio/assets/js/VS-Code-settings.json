{
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 12,
    "workbench.colorCustomizations": {
        "window.activeBorder": "#ff0000",
        "window.inactiveBorder": "#ddd",
        "editor.background": "#0f0f0f",
        "editor.selectionBackground": "#00ffdd30",
        "editor.selectionHighlightBackground": "#00ffdd20",
        "editor.selectionHighlightBorder": "#00ffdd30",
        "editor.wordHighlightBackground": "#1e1e1e00",
        "editor.wordHighlightStrongBackground": "#1e1e1e00",
        "editor.lineHighlightBorder": "#335ed644",
        "editor.findMatchBorder": "#73ff00",
        "editor.findMatchHighlightBackground": "#73ff0036",
        "editor.findMatchHighlightBorder": "#73ff0075",
        "editorWhitespace.foreground": "#ff8b8b56",
        "editorIndentGuide.activeBackground": "#335ed6",
        "editorIndentGuide.background": "#335ed655"
    },
    "workbench.iconTheme": "vscode-icons",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "pigments.enabledExtensions": "css, scss, sass, less, html, js, json, jsx, vue, xml, dwp",
    "pigments.markerType": "background",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#788d6d",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.language",
                    "text.html.derivative",
                    "JSXNested"
                ],
                "settings": {
                    "foreground": "#ffffff"
                }
            },
            {
                "scope": [
                    "keyword.control.conditional.js",
                    "keyword.control.trycatch.js"
                ],
                "settings": {
                    "foreground": "#89DDFF"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "entity.name.function.js",
                    "entity.name.class.js",
                    "support.class.component.open.jsx",
                    "entity.name.type.class.js"
                ],
                "settings": {
                    "foreground": "#82AAFF"
                }
            },
            {
                "scope": [
                    "string.quoted.double.js",
                    "string.quoted.single.js",
                    "string.quoted.module.js"
                ],
                "settings": {
                    "foreground": "#C3E88D"
                }
            },
            {
                "scope": [
                    "string.unquoted.js",
                    "meta.object-literal.key.js"
                ],
                "settings": {
                    "foreground": "#B5D8F6"
                }
            },
            {
                "scope": "support.type.object.console.js",
                "settings": {
                    "foreground": "#ffd283",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "support.type.object.dom.js",
                "settings": {
                    "foreground": "#F78C6C"
                }
            },
            {
                "scope": "storage.type.function.js",
                "settings": {
                    "foreground": "#C792EA",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                    "punctuation.definition.entity.css"
                ],
                "settings": {
                    "foreground": "#00f7ff"
                }
            },
            {
                "scope": [
                    "variable.other.property"
                ],
                "settings": {
                    "foreground": "#00d6ba"
                }
            },
            {
                "scope": [
                    "support.constant.font-name.css"
                ],
                "settings": {
                    "foreground": "#B5CEA8"
                }
            },
            {
                "scope": [
                    "support.constant.property-value.css",
                    "meta.property-value.css"
                ],
                "settings": {
                    "foreground": "#B5CEA8"
                }
            },
            {
                "scope": [
                    "entity.name.tag.css",
                    "support.function.url.css",
                    "support.function.misc.css"
                ],
                "settings": {
                    "foreground": "#BD63C5",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name.pseudo-class.css",
                    "entity.other.attribute-name.pseudo-element.css"
                ],
                "settings": {
                    "foreground": "#F78C6C", //"#ef3aff",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "keyword.other.unit"
                ],
                "settings": {
                    "foreground": "#569CD6"
                }
            },
            {
                "scope": "keyword.other.important.css",
                "settings": {
                    "foreground": "#00d6ba"
                }
            },
            {
                "scope": [
                    "punctuation.terminator.rule.css"
                ],
                "settings": {
                    "foreground": "#335ed6"
                }
            },
            {
                "name": "brackets of XML/HTML tags",
                "scope": [
                    "punctuation.definition.tag",
                    "entity.other.attribute-name.html",
                    "entity.other.attribute-name.localname.xml"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name.class.css",
                    "entity.other.attribute-name.class.mixin.css",
                    "entity.other.attribute-name.id.css",
                    "entity.other.attribute-name.parent-selector.css",
                    "source.css.less entity.other.attribute-name.id",
                    "entity.other.attribute-name.attribute.scss",
                    "entity.other.attribute-name.scss"
                ],
                "settings": {
                    "foreground": "#ffca58",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "this.self",
                "scope": [
                    "variable.language.this.js",
                    "variable.language.self.js"
                ],
                "settings": {
                    "foreground": "#fcae96",
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": [
                    "punctuation.separator.key-value.html",
                    "punctuation.separator.key-value.css",
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end",
                    "punctuation.definition.entity.html"
                ],
                "settings": {
                    "foreground": "#569CD6"
                }
            },
            {
                "scope": [
                    "constant.character",
                    "entity.name.tag.html.jsx"
                ],
                "settings": {
                    "foreground": "#2FA0DD",
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": [
                    "constant.numeric.js",
                    "constant.language.boolean.true.js",
                    "constant.language.boolean.false.js"
                ],
                "settings": {
                    "foreground": "#FF628C"
                }
            },
            {
                "scope": [
                    "variable.other.object.js",
                    "variable.other.readwrite.js",
                    "variable.other.class.js",
                    "variable.parameter.js",
                    "variable.other.constant.js"
                ],
                "settings": {
                    "foreground": "#eaf5ff"
                }
            },
            {
                "scope": "constant.other.color",
                "settings": {
                    "foreground": "#FFFFFF"
                }
            },
            {
                "scope": [
                    "keyword.operator.new",
                    "keyword.operator.expression",
                    "keyword.operator.cast",
                    "keyword.operator.sizeof",
                    "keyword.operator.instanceof",
                    "keyword.operator.logical.python"
                ],
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "scope": "string.regexp",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "name": "String interpolation",
                "scope": [
                    "punctuation.definition.template-expression.begin",
                    "punctuation.definition.template-expression.end",
                    "punctuation.section.embedded"
                ],
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "name": "Reset JavaScript string interpolation expression",
                "scope": [
                    "meta.template.expression"
                ],
                "settings": {
                    "foreground": "#d4d4d4"
                }
            },
            {
                "scope": [
                    "support.type.vendored.property-name",
                    "variable.css",
                    "variable.scss",
                    "variable.other.less",
                    "source.coffee.embedded"
                ],
                "settings": {
                    "foreground": "#9cdcfe"
                }
            }
        ]
    },
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "off",
    "editor.wordWrap": "on",
    "files.associations": {
        "*.tag": "html",
        "*.cshtml": "html",
        "*.html": "html",
        "*.njk": "html",
        "*.mustache": "html",
        "*.html.md": "html",
        "*.keys": "html",
        "*.dwp": "html",
        "*.dwc": "html"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/.vscode": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/node_modules": true,
        "node_modules": true,
        "venv": true,
        "*.sublime-*": true,
        "env*": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/env": true,
        "**/venv": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/env/**": true,
        "**/venv/**": true,
        "env-*": true
    },
    "breadcrumbs.enabled": true,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "workbench.colorTheme": "Community Material Theme Darker High Contrast"
}